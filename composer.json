{
    "name": "ferdousanam/adminlte3",
    "type": "library",
    "description": "This package is a wrapper of adminlte 3 which includes Laravel AdminLTE 3 Scaffold Generator",
    "keywords": [
        "laravel",
        "adminlte3",
        "adminlte3 scaffold generator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ferdous Anam",
            "email": "ferdous.anam@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "laravel/laravel": "*",
        "almasaeed2010/adminlte": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Anam\\Adminlte\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anam\\Adminlte\\AdminlteServiceProvider"
            ]
        }
    }
}
